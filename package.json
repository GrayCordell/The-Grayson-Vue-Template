{
  "name": "package.json",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.2",
  "engines": {
    "pnpm": ">=9.12.2"
  },
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "buildForDebug": "vite build --minify false --emptyOutDir --debug",
    "build-only": "vite build",
    "dev": "vite --port 5001",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "npx simple-git-hooks",
    "preview": "vite preview",
    "sizecheck": "npx vite-bundle-visualizer",
    "sizecheck:dash": "npx vite-bundle-visualizer --config vite.dashboard.config.ts",
    "test": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "test:unit": "vitest",
    "type-check": "vue-tsc --build",
    "up": "taze major -I"
  },
  "dependencies": {
    "@imengyu/vue3-context-menu": "^1.4.3",
    "@unhead/vue": "^1.11.18",
    "@vicons/ionicons5": "^0.13.0",
    "@vueuse/components": "^11.0.3",
    "@vueuse/core": "^11.0.3",
    "@vueuse/integrations": "^11.1.0",
    "@vueuse/math": "^11.0.3",
    "@vueuse/sound": "^2.0.1",
    "axios": "^1.7.9",
    "i18next": "^23.16.0",
    "jenesius-vue-modal": "^1.11.9",
    "lodash-es": "^4.17.21",
    "lz-string": "^1.5.0",
    "naive-ui": "^2.40.1",
    "pinia": "^3.0.1",
    "uuid": "^10.0.0",
    "vue": "^3.5.13",
    "vue-base-tooltip": "^1.2.0",
    "vue-dompurify-html": "^5.2.0",
    "vue-i18n": "10.0.5",
    "vue-router": "^4.4.5",
    "vue3-simple-alert": "^1.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.0",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@types/node": "^22.12.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@volar/language-server": "^2.4.11",
    "@vue/language-server": "^2.2.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "cypress": "^14.1.0",
    "cypress-vite": "^1.6.0",
    "eslint": "^9.21.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-scoped-construct": "^0.2.1",
    "https-localhost": "^4.7.1",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "postcss": "^8.5.1",
    "postcss-html": "^1.8.0",
    "postcss-nested": "^7.0.2",
    "postcss-simple-vars": "^7.0.1",
    "sass": "^1.83.4",
    "sharp": "^0.33.5",
    "simple-git-hooks": "^2.11.1",
    "svgo": "^3.3.2",
    "taze": "^18.3.0",
    "type-fest": "^4.33.0",
    "typescript": "^5.7.3",
    "unplugin-icons": "^22.0.0",
    "vite": "^6.0.11",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-inspect": "^10.1.0",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vite-plugin-vue-inspector": "^5.3.1",
    "vite-plugin-webfont-dl": "^3.10.4",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "start-server-and-test": "^2.0.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
